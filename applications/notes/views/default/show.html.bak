{{extend 'layout.html'}}
 
<form enctype="multipart/form-data" action="{{URL()}}" method="post">
    <select name='year' 
        onchange="jQuery('#maker_name').empty();
        ajax('maker', ['year'], 'maker_name');">
        <option>Select Year</option>
        {{for i in dict_test:}}
            <option value="{{=i}}">
            {{=i}}
            </option>
        {{pass}}
    </select>

    <select id='maker_name' name='month' onchange="
        ajax('one', ['month'], 'show_list');" >
        <option>Select Month</option>
        {{for maker in makers:}}
            <option value="{{=maker}}">
            {{=maker}}</option>
        {{pass}}
    </select>
</form>
<div id="show_list">
{{import datetime}}

{{for i in html_list:}}
{{for j,k in i.iteritems():}}
<pre><b>{{=j}}</b>
{{=k}}</pre><br><br>
{{pass}}
{{pass}}
    </div>
